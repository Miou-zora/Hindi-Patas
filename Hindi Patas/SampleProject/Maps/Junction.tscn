resource_name = "Config"
script/source = "# This is a special config node that determines the scene's content.  The \"content\" in this case are the available exits.
extends Node
# This variable is set when the scene is being \"generated\" (see DiceRoom.tscn).
@export var exits: int

# When the player enters the room, remove the walls based on the assigned exits.
func _ready() -> void:
	for i in 4:
		%TileMap.set_layer_enabled(i + 2, not bool(exits & 1 << i))
"

